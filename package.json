{
  "name": "jsfeatures.in",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > public/bundle.js",
    "serve": "ecstatic -p 8000 .",
    "watch": "watchify main.js -o public/bundle.js -dv",
    "preinstall": "npm i -g uglifyjs"
  },
  "dependencies": {
    "babelify": "6.3.0",
    "browserify": "10.2.6",
    "ecstatic": "0.8.0",
    "highlight.js": "8.8.0",
    "js-beautify": "1.5.10",
    "material-ui": "0.10.4",
    "react": "0.13.3",
    "react-highlight": "0.5.0",
    "react-tap-event-plugin": "0.1.8",
    "uglify": "0.1.5",
    "watchify": "3.4.0"
  }
}
